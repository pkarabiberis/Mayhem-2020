<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_marginVertical="25dp"
	android:id="@+id/testaajaLeiska">

	<TextView
		android:id="@+id/testeriPosition_textView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAlignment="center"
		android:layout_marginVertical="5dp"
		/>

	<TextView
		android:id="@+id/testeriName_textView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAlignment="center"
		android:layout_marginVertical="5dp"
		/>

	<TextView
		android:id="@+id/testeriTime_textView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAlignment="center"
		android:layout_marginVertical="5dp"
		/>

	<TextView
		android:id="@+id/testeriSponsor_textView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAlignment="center"
		android:layout_marginVertical="5dp"
		/>

	<TextView
		android:id="@+id/testeriClub_textView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAlignment="center"
		android:layout_marginVertical="5dp"
		/>

	<TextView
		android:id="@+id/testeriAthleteNumber_textView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAlignment="center"
		android:layout_marginVertical="5dp"
		/>
</LinearLayout>